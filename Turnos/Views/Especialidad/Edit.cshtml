@model Turnos.Models.Especialidad
@{
    ViewBag.Title = "Especialidad";
}
<h4>Editar especialidad</h4>
<hr />
<div class="row">
    <div>
        <form class="col s12" asp-action="Edit">
            <div asp-validation-summary="ModelOnly" ></div>
            <input type="hidden" asp-for="IdEspecialidad" />
            <div class="row">
              <div class="input-field col s6">
                    <i class="material-icons prefix">star_rate</i>
                    <input asp-for="Descripcion" type="text" class="validate">
                    <label asp-for="Descripcion"></label>
                    <span asp-validation-for="Descripcion" ></span>
              </div>
            </div>
            <div class="row">
                <div class="form-group col s2 m2 offset-s8">
                    <button class="btn waves-effect waves-light" type="submit" name="action">Guardar
                      <i class="material-icons right">send</i>
                    </button>
                </div>
                <div class="col s2 m2">
                    <a asp-action="Index" class="waves-effect waves-light btn grey darken-4"><i class="material-icons right">undo</i>button</a>
                </div>
            </div>
        </form>
    </div>
</div>
